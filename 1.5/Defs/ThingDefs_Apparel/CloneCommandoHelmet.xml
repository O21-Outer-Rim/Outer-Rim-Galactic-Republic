<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<ThingDef ParentName="OuterRim_HeavyHelmetFabricatedBase">
		<defName>OuterRim_CloneCommandoHelmet</defName>
		<label>clone commando helmet</label>
		<description>An armoured helmet used by the Clone Commandos as part of the Grand Army of the Republic.</description>
		<graphicData>
			<texPath>OuterRim/Apparel/CloneCommando/Helmet/Head</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<color>(255, 255, 255)</color>
		</graphicData>
		<equippedStatOffsets>
			<ToxicEnvironmentResistance>0.3</ToxicEnvironmentResistance>
		</equippedStatOffsets>
		<apparel>
			<immuneToToxGasExposure>true</immuneToToxGasExposure>
			<tags Inherit="False">
				<li>RepublicCloneApparel</li>
				<li>RepublicCommando</li>
			</tags>
			<wornGraphicPath>OuterRim/Apparel/CloneCommando/Helmet/Head</wornGraphicPath>
			<useWornGraphicMask>true</useWornGraphicMask>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options" Inherit="False">
			<options>
				<li>
					<weight>10</weight>
					<only>(255, 255, 255)</only>
				</li>
			</options>
		</colorGenerator>
	</ThingDef>

</Defs>