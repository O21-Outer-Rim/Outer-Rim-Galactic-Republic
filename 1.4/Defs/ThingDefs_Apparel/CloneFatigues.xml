<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="OuterRim_UniformBodyFabricatedBase">
		<defName>OuterRim_CloneTrooperFatigues</defName>
		<label>clone trooper fatigues</label>
		<description>A durable training uniform used by clones of the Republic.</description>
		<graphicData>
			<texPath>OuterRim/Apparel/CloneFatigues/Apparel</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<color>(181, 94, 94)</color>
		</graphicData>
		<recipeMaker>
			<researchPrerequisite>OuterRim_RepublicArmory</researchPrerequisite>
		</recipeMaker>
		<apparel>
			<tags Inherit="False">
				<li>ORCloneFatigues</li>
			</tags>
			<wornGraphicPath>OuterRim/Apparel/CloneFatigues/Apparel</wornGraphicPath>
			<useWornGraphicMask>false</useWornGraphicMask>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>(181, 94, 94)</only>
				</li>
			</options>
		</colorGenerator>
	</ThingDef>

</Defs>